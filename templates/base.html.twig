<!DOCTYPE html>

<html>

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="UTF-8">
    <title>{% block title %}Wild series{% endblock %}</title>
    {% block stylesheets %}
        <link href="https://fonts.googleapis.com/css?family=Orbitron|Notable|Abel&display=swap" rel="stylesheet">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/uikit@3.3.0/dist/css/uikit.min.css" />
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    <link rel="shortcut icon" type="image/png" href="{{ asset('build/images/wild-series.ico') }}"/>
</head>

<header>
    {% block navbar %}
        {% include "_navbar.html.twig" %}
    {% endblock %}
</header>

<body>

{% block body %}{% endblock %}
{% block javascripts %}
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.3.0/dist/js/uikit.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.3.0/dist/js/uikit-icons.min.js"></script>
    {{ encore_entry_script_tags('app') }}
{% endblock %}

</body>

</html>
